[{
        "description": "Lista comenzilor Consolei Mautic",
        "command": "/var/www/mautic/bin/console list"
    },
    {
        "description": "Restabileşte permisiunile dosarului Mautic",
        "command": "bash /usr/local/bin/reset-permisiuni-mautic.sh"
    },
    {
        "description": "Şterge cache-ul",
        "command": "/var/www/mautic/bin/console cache:clear"
    },
    {
        "description": "Crează acum o copie de rezervă a mautic (baza de date şi dosar Mautic)",
        "command": "bash /var/mautic-crons/cron-backup.sh"
    },
    {
        "description": "Actualizează toate segmentele",
        "command": "/var/www/mautic/bin/console mautic:segments:update"
    },
    {
        "description": "Actualizează toate campaniile",
        "command": "/var/www/mautic/bin/console mautic:campaigns:update"
    },
    {
        "description": "Proceseaza toate campaniile",
        "command": "/var/www/mautic/bin/console mautic:campaigns:trigger"
    },
    {
        "description": "Trimite emailurile",
        "command": "/var/www/mautic/bin/console mautic:emails:send"
    },
    {
        "description": "Trimite newsletterele",
        "command": "/var/www/mautic/bin/console mautic:broadcasts:send"
    },
    {
        "description": "Trimite SMS-urile",
        "command": "/var/www/mautic/bin/console mautic:messages:send"
    },
    {
        "description": "Proceseaza webhook-urile",
        "command": "/var/www/mautic/bin/console mautic:webhooks:process"
    },
    {
        "description": "Proceseaza rapoartele programate",
        "command": "/var/www/mautic/bin/console mautic:reports:scheduler"
    },
    {
        "description": "Actualizeaza plugin-urile",
        "command": "/var/www/mautic/bin/console mautic:plugins:update"
    },
    {
        "description": "Importa 600 contacte",
        "command": "/var/www/mautic/bin/console mautic:import --limit=600"
    },
    {
        "description": "Arata info mai vechi de 90 de zile ce pot fi sterse",
        "command": "/var/www/mautic/bin/console mautic:maintenance:cleanup --no-interaction --days-old=90 --dry-run"
    },
    {
        "description": "Sterge info mai vechi de 90 de zile",
        "command": "/var/www/mautic/bin/console mautic:maintenance:cleanup --no-interaction --days-old=90"
    },
    {
        "description": "Deduplicarea contactelor",
        "command": "/var/www/mautic/bin/console mautic:contacts:deduplicate"
    },
    {
        "description": "Şterge IP-urile nefolosite",
        "command": "/var/www/mautic/bin/console mautic:unusedip:delete"
    },
    {
        "description": "Actualizează baza de date MaxMind",
        "command": "/var/www/mautic/bin/console mautic:iplookup:download"
    },
    {
        "description": "Vezi starea migrărilor",
        "command": "/var/www/mautic/bin/console doctrine:migrations:status"
    },
    {
        "description": "Validează starea migrărilor",
        "command": "/var/www/mautic/bin/console doctrine:schema:validate"
    },
    {
        "description": "Afişează comenzile SQL pentru a actualiza baza de date",
        "command": "/var/www/mautic/bin/console doctrine:schema:update --dump-sql"
    },
    {
        "description": "Resetează statistica emailurilor de la Webinarii",
        "command": "/var/www/mautic/bin/console doctrine:query:sql \"UPDATE emails SET read_count = 0, sent_count = 0, variant_sent_count = 0, variant_read_count = 0 WHERE id IN (SELECT e.id FROM emails e JOIN categories c ON e.category_id = c.id WHERE LOWER(c.title) LIKE '%webinar%');\""
    },
    {
        "description": "Şterg email_stats pentru emailurile de Webinarii",
        "command": "/var/www/mautic/bin/console doctrine:query:sql \"DELETE FROM email_stats WHERE email_id IN (SELECT e.id FROM emails e JOIN categories c ON e.category_id = c.id WHERE LOWER(c.title) LIKE '%webinar%');\""
    },
    {
        "description": "Actualizează acest utilitar",
        "command": "bash /var/www/mautic/comenzi.sh"
    }
]